	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.33.0",
		"@types/node": "^24.3.0",
		"@typescript-eslint/eslint-plugin": "^8.40.0",
		"@typescript-eslint/parser": "^8.40.0",
		"eslint": "^9.33.0",
		"globals": "^16.3.0",
		"typescript": "^5.9.2",
		"wrangler": "^4.32.0"
